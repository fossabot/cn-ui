{
    "name": "@cn-ui/core",
    "version": "2.4.0",
    "type": "module",
    "main": "./dist/lib/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "keywords": [
        "cn-ui",
        "component",
        "components",
        "design",
        "framework",
        "frontend",
        "solid",
        "solid-js",
        "solid-component",
        "solid-js-component",
        "ui"
    ],
    "bugs": {
        "url": "https://github.com/KonghaYao/cn-ui/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/KonghaYao/cn-ui"
    },
    "homepage": "https://cn-ui.netlify.app",
    "license": "MIT",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "sideEffects": [
        "**/*.css"
    ],
    "scripts": {
        "build": "node ./script/clean-dist.mjs && node ./script/build-entry.mjs && tsc && pnpm build:css && pnpm docgen",
        "build:css": "node ./script/copy-css-to-dist.mjs && unocss \"src/**/*.{ts,tsx,css}\" --out-file dist/cn-uno.css -m --preflights && unocss \"dist/lib/**/*.css\" --write-transformed -m --out-file dist/cn-uno.not.css",
        "docgen": "node ./script/docgen.mjs",
        "storybook": "storybook dev -p 6006",
        "type-check": "tsc -p tsconfig.node.json",
        "prettier": "prettier --write src/**/*",
        "prepublish": "pnpm build"
    },
    "dependencies": {
        "@ark-ui/solid": "^2.2.0",
        "@cn-ui/reactive": "workspace:^",
        "@cn-ui/sortable": "workspace:^",
        "@cn-ui/svg-spinner": "workspace:^",
        "@popperjs/core": "^2.11.8",
        "@solid-primitives/keyed": "^1.2.2",
        "@storybook/test": "^8.0.4",
        "@tanstack/solid-table": "^8.15.3",
        "@tanstack/solid-virtual": "^3.2.0",
        "@tanstack/table-core": "^8.15.3",
        "@tanstack/virtual-core": "^3.2.0",
        "@types/lodash": "^4.17.0",
        "@types/lodash-es": "^4.17.12",
        "async-validator": "^4.2.5",
        "copy-to-clipboard": "^3.3.3",
        "esbuild": "^0.20.2",
        "lodash-es": "^4.17.21",
        "solid-icons": "^1.1.0",
        "solid-js": "^1.8.16",
        "solid-transition-group": "^0.2.3",
        "solidjs-use": "^2.3.0",
        "split.js": "^1.6.5",
        "ts-toolbelt": "^9.6.0",
        "viewerjs": "^1.11.6"
    },
    "devDependencies": {
        "@alenaksu/json-viewer": "^2.0.1",
        "@ant-design/colors": "^7.0.2",
        "@cn-ui/area-data": "workspace:^",
        "@testing-library/jest-dom": "^6.4.2",
        "@types/sortablejs": "^1.15.8",
        "@unocss/cli": "^0.58.6",
        "@unocss/reset": "^0.58.6",
        "@unocss/transformer-directives": "^0.58.6",
        "@vant/touch-emulator": "^1.4.0",
        "fuse.js": "^7.0.0",
        "glob": "^10.3.10",
        "jsdom": "^22.1.0",
        "mock-type": "workspace:^",
        "mockjs-ts": "^2.3.0",
        "react-docgen-typescript": "^2.2.2",
        "runes2": "^1.1.4",
        "unocss": "^0.58.6",
        "unocss-preset-chinese": "^0.3.2",
        "unocss-preset-theme": "^0.12.0"
    }
}
