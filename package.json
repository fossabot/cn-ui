{
    "name": "cn-ui",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "main": "./dist/index.js",
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "storybook": "storybook dev -p 6006",
        "type-check": "tsc -p tsconfig.node.json",
        "check": "biome check --apply ./ --diagnostic-level=error",
        "build-storybook": "pnpm --filter=@cn-ui/area-data build && pnpm --filter=@cn-ui/svg-spinner build && storybook build",
        "prettier": "prettier --write packages/**/*.{json,less,md}",
        "build": "pnpm --filter \"*\" build && pnpm storybook build"
    },
    "dependencies": {
        "@cn-ui/area-data": "workspace:^",
        "@cn-ui/reactive": "workspace:^",
        "@cn-ui/univer": "workspace:^",
        "solid-js": "^1.8.16"
    },
    "devDependencies": {
        "@ant-design/colors": "^7.0.2",
        "@biomejs/biome": "^1.7.0",
        "@chromatic-com/storybook": "^1.3.3",
        "@joshwooding/vite-plugin-react-docgen-typescript": "^0.3.1",
        "@solidjs/testing-library": "^0.8.7",
        "@storybook/addon-a11y": "^8.0.8",
        "@storybook/addon-essentials": "^8.0.8",
        "@storybook/addon-interactions": "^8.0.8",
        "@storybook/addon-links": "^8.0.8",
        "@storybook/addon-mdx-gfm": "^8.0.8",
        "@storybook/blocks": "^8.0.8",
        "@storybook/test": "^8.0.8",
        "@storybook/test-runner": "^0.17.0",
        "@testing-library/jest-dom": "^6.4.2",
        "@types/lodash": "^4.17.0",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^18.19.31",
        "@types/sortablejs": "^1.15.8",
        "@unocss/cli": "^0.58.9",
        "@unocss/reset": "^0.58.9",
        "@unocss/transformer-directives": "^0.58.9",
        "@vant/touch-emulator": "^1.4.0",
        "fs-extra": "^11.2.0",
        "glob": "^10.3.12",
        "hast-util-to-jsx-runtime": "^2.3.0",
        "jsdom": "^22.1.0",
        "lint-staged": "^15.2.2",
        "lodash-es": "^4.17.21",
        "mockjs-ts": "^2.3.0",
        "release-it": "^17.2.0",
        "rollup-plugin-bundle-stats": "^4.12.2",
        "rollup-plugin-multi-input": "^1.4.1",
        "runes2": "^1.1.4",
        "storybook": "^8.0.8",
        "storybook-solidjs": "1.0.0-beta.2",
        "storybook-solidjs-docgen": "workspace:^",
        "storybook-solidjs-vite": "1.0.0-beta.2",
        "typescript": "^5.4.5",
        "unocss": "^0.58.9",
        "unocss-preset-chinese": "^0.3.2",
        "unocss-preset-theme": "^0.12.0",
        "vite": "^5.2.10",
        "vite-bundle-analyzer": "^0.9.4",
        "vite-plugin-dts": "^3.8.3",
        "vite-plugin-solid": "^2.10.2",
        "vite-plugin-wasm": "^3.3.0",
        "vitest": "^1.5.0",
        "yorkie": "^2.0.0"
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx,json}": "biome check --diagnostic-level=error  ",
        "*.{css,md,mdx}": "prettier --ignore-unknown "
    },
    "gitHooks": {
        "pre-commit": "lint-staged"
    }
